{%- style -%}

  {{  heading_font | font_face: font_display: 'swap' }}
  {{  author_font | font_face: font_display: 'swap' }}
  {{  place_font | font_face: font_display: 'swap' }}
  {{  time_font | font_face: font_display: 'swap' }}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .insta-heading-{{ section.id }} {
    width: 100%;
    text-align: {{ heading_align_mobile }};
  }

  .insta-heading-{{ section.id }} * {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: {{ heading_height }}%;
    text-transform: unset;
  }

  .insta-heading-{{ section.id }} a {
    text-decoration: none;
    font-size: {{ heading_size_mobile | times: 0.75 | round: 0 }}px;
  }

  .insta-items-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: {{ items_margin_mobile }}px;
    flex-direction: column;
    justify-content: center;
  }

  .insta-slider-wrapper-{{ section.id }} {
    width: 100%;
    max-width: {{ items_width_mobile }}%;
    margin: 0px auto;
    min-width: 0;
    position: relative;
  }
  
  .insta-slider-{{ section.id }} .swiper-wrapper {
    align-items: stretch;
  }

  .insta-slide-{{ section.id }} {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: {{ card_padding_vertical_mobile }}px {{ card_padding_horizontal_mobile }}px;
    border-radius: {{ card_radius }}px;
    border: {{ card_border_thickness }}px solid {{ card_border_color }};
    background-color: {{ card_bg_color }};
    box-sizing: border-box !important; 
    height: auto !important;
    text-decoration: none;
    overflow: hidden;
    height: auto !important;
  }

  .insta-top-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .insta-dots-{{ section.id }} {
    margin-left: auto;
  }

  .insta-dots-{{ section.id }} svg path {
    stroke: {{ dots_color }};
  }

  .insta-logo-{{ section.id }} {
    flex: 0 0 {{ logo_size_mobile }}px;
    height: {{ logo_size_mobile }}px;
    border-radius: 50%;
    overflow: hidden;
    background-color: #DDDDDD;
  }

  .insta-logo-{{ section.id }} img {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .insta-author-{{ section.id }} {
    margin: 0;
    font-size: {{ author_size_mobile }}px;
    color: {{ author_color }};
    line-height: {{ author_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .insta-place-{{ section.id }} {
    margin: 0;
    margin-top: {{ place_mt | times: 0.75 | round: 0 }}px;
    font-size: {{ place_size_mobile }}px;
    color: {{ place_color }};
    line-height: {{ place_height }}%;
    text-transform: unset;
  }

  .insta-image-{{ section.id }} {
    margin-top: {{ image_mt | times: 0.75 | round: 0 }}px;
    border-radius: {{ image_radius }}px;
    border: {{ image_border_thickness }}px solid {{ image_border_color }};
  }

  .insta-image-{{ section.id }} img,
  .insta-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .insta-image-{{ section.id }} svg {
    background-color: #DDDDDD;
  }

  .insta-actions-{{ section.id }} {
    display: flex;
    aling-items: center;
    gap: {{ actions_gap_mobile }}px;
    margin-top: {{ actions_mt | times: 0.75 | round: 0 }}px;
  }

  .insta-actions-{{ section.id }} svg {
    display: block;
    width: {{ actions_icon_size_mobile }}px;
    height: {{ actions_icon_size_mobile }}px;
  }

  .insta-actions-{{ section.id }} svg:last-child {
    margin-left: auto;
  }

  .insta-actions-{{ section.id }} + .insta-author-{{ section.id }} {
    margin-top: {{ author_mt | times: 0.75 | round: 0 }}px;
  }

  .insta-time-{{ section.id }} {
    margin: 0;
    margin-top: {{ time_mt | times: 0.75 | round: 0 }}px;
    font-size: {{ time_size_mobile }}px;
    color: {{ time_color }};
    line-height: {{ time_height }}%;
    text-transform: unset;
  }

  .insta-btn-prev-{{ section.id }},
  .insta-btn-next-{{ section.id }} {
    position: absolute;
    z-index: 2;
    top: 50%;
    transform: translateY(-50%);
    display: flex;
    align-items: center;
    justify-content: center;
    width: {{ arrow_size_mobile }}px;
    height: {{ arrow_size_mobile }}px;
    border: {{ arrow_border_thickness }}px solid {{ arrow_border_color }};
    border-radius: 50%;
    background: {{ arrow_bg_color }};
    align-items: center;
    justify-content: center;
    transition: all 0.3s ease 0s;
    cursor: pointer;
    -webkit-appearance: none;
  }

  .insta-btn-prev-{{ section.id }} {
    left: 0px !important;
  }

  .insta-btn-next-{{ section.id }} {
    right: 0px !important;
  }

  .insta-btn-prev-{{ section.id }} svg,
  .insta-btn-next-{{ section.id }} svg {
    height: {{ arrow_icon_size_mobile }}px;
    width: {{ arrow_icon_size_mobile }}px;
  }

  .insta-btn-next-{{ section.id }} svg {
    transform: rotate(270deg);
    margin-left: 2px;
  }

  .insta-btn-prev-{{ section.id }} svg {
    transform: rotate(90deg);
    margin-left: -2px;
  }

  .insta-btn-prev-{{ section.id }} svg path,
  .insta-btn-next-{{ section.id }} svg path {
    fill: {{ arrow_color }};
    transition: all 0.3s ease 0s;
  }

  .insta-btn-prev-{{ section.id }}:hover ,
  .insta-btn-next-{{ section.id }}:hover {
    transition: all 0.3s ease 0s;
    border: {{ arrow_border_thickness }}px solid {{ arrow_border_hover_color }};
    background: {{ arrow_bg_hover_color }};
  }

  .insta-btn-prev-{{ section.id }}:hover svg path,
  .insta-btn-next-{{ section.id }}:hover svg path {
    fill: {{ arrow_hover_color }};
    transition: all 0.3s ease 0s;
  }

  .insta-btn-prev-{{ section.id }}.swiper-button-disabled,
  .insta-btn-next-{{ section.id }}.swiper-button-disabled {
    opacity: 0;
    transition: all 0.3s ease 0s;
  }

  .insta-scrollbar-{{ section.id }} {
    margin-top: {{ scrollbar_mt | times: 0.75 | round: 0 }}px;
    position: static !important;
    height: {{ scrollbar_height }}px !important;
    background-color: {{ scrollbar_bg_color }}!important;
  }

  .insta-scrollbar-{{ section.id }} .swiper-scrollbar-drag {
    display: block !important;
    background-color: {{ scrollbar_drag_bg_color }}!important;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .insta-heading-{{ section.id }} {
      flex: 0 0 {{ 100 | minus: items_width }}%;
      text-align: {{ heading_align }};
    }

    .insta-heading-{{ section.id }} * {
      font-size: {{ heading_size }}px;
    }

    .insta-heading-{{ section.id }} a {
      font-size: {{ heading_size | times: 0.75 | round: 0 }}px;
    }

    .insta-items-{{ section.id }} {
      flex-direction: row;
      gap: {{ items_margin }}px;
    }

    .insta-slider-wrapper-{{ section.id }} {
      max-width: 100%;
      flex: 0 0 {{ items_width }}%;
    }

    .insta-slide-{{ section.id }} {
      padding: {{ card_padding_vertical }}px {{ card_padding_horizontal }}px;
    }

    .insta-logo-{{ section.id }} {
      flex: 0 0 {{ logo_size }}px;
      height: {{ logo_size }}px;
    }

    .insta-author-{{ section.id }} {
      font-size: {{ author_size }}px;
    }

    .insta-place-{{ section.id }} {
      margin-top: {{ place_mt }}px;
      font-size: {{ place_size }}px;
    }

    .insta-image-{{ section.id }} {
      margin-top: {{ image_mt }}px;
    }

    .insta-actions-{{ section.id }} {
      gap: {{ actions_gap }}px;
      margin-top: {{ actions_mt }}px;
    }

    .insta-actions-{{ section.id }} svg {
      width: {{ actions_icon_size }}px;
      height: {{ actions_icon_size }}px;
    }

    .insta-actions-{{ section.id }} + .insta-author-{{ section.id }} {
      margin-top: {{ author_mt }}px;
    }

    .insta-time-{{ section.id }} {
      margin-top: {{ time_mt }}px;
      font-size: {{ time_size }}px;
    }

    .insta-btn-prev-{{ section.id }},
    .insta-btn-next-{{ section.id }} {
      width: {{ arrow_size }}px;
      height: {{ arrow_size }}px;
    }

    .insta-btn-prev-{{ section.id }} {
      left: -{{ arrow_size | times: 0.5 }}px !important;
    }
  
    .insta-btn-next-{{ section.id }} {
      right: -{{ arrow_size | times: 0.5 }}px !important;
    }

    .insta-btn-prev-{{ section.id }} svg,
    .insta-btn-next-{{ section.id }} svg {
      height: {{ arrow_icon_size }}px;
      width: {{ arrow_icon_size }}px;
    }

    .insta-scrollbar-{{ section.id }} {
      margin-top: {{ scrollbar_mt }}px;
    }
    
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}rem;
    }
  </style>
{% endunless %}

{% if heading_custom %}
  <style>
    .insta-heading-{{ section.id }} * {
      font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
      font-weight: {{ heading_font.weight }};
      font-style: {{ heading_font.style }};
    }
  </style>
{% endif %}

{% if author_custom %}
  <style>
    .insta-author-{{ section.id }} {
      font-family: {{ author_font.family }}, {{ author_font.fallback_families }};
      font-weight: {{ author_font.weight }};
      font-style: {{ author_font.style }};
    }
  </style>
{% endif %}

{% if place_custom %}
  <style>
    .insta-place-{{ section.id }} {
      font-family: {{ place_font.family }}, {{ place_font.fallback_families }};
      font-weight: {{ place_font.weight }};
      font-style: {{ place_font.style }};
    }
  </style>
{% endif %}

{% if time_custom %}
  <style>
    .insta-time-{{ section.id }} {
      font-family: {{ time_font.family }}, {{ time_font.fallback_families }};
      font-weight: {{ time_font.weight }};
      font-style: {{ time_font.style }};
    }
  </style>
{% endif %}

{% unless heading_row %}
  <style>
    .insta-heading-{{ section.id }} {
      margin-bottom: {{ items_margin_mobile }}px;
    }

    @media(min-width: 1024px) {
      .insta-heading-{{ section.id }} {
        margin-bottom: {{ items_margin }}px;
      }
    }
  </style>
{% endunless %}

{% if image_ratio_mobile == "portrait" %}
  <style>
    .insta-image-{{ section.id }} {
     aspect-ratio: 9.6/11; 
    }
  </style>
{% elsif image_ratio_mobile == "landscape" %}
  <style>
    .insta-image-{{ section.id }} {
     aspect-ratio: 12/7; 
    }
  </style>
{% elsif image_ratio_mobile == "square" %}
  <style>
    .insta-image-{{ section.id }} {
     aspect-ratio: 12/12; 
    }
  </style>
{% endif %}

{% if image_ratio == "portrait" %}
  <style>
    @media(min-width: 1024px) {
     .insta-image-{{ section.id }} {
       aspect-ratio: 9.6/11; 
      } 
    }
  </style>
{% elsif image_ratio == "landscape" %}
  <style>
    @media(min-width: 1024px) {
      .insta-image-{{ section.id }} {
       aspect-ratio: 12/7; 
      }
    }
  </style>
{% elsif image_ratio == "square" %}
  <style>
    @media(min-width: 1024px) {
      .insta-image-{{ section.id }} {
       aspect-ratio: 12/12; 
      }
    }
  </style>
{% endif %}

{% if arrow_show_mobile %}
  <style>
    .insta-btn-prev-{{ section.id }},
    .insta-btn-next-{{ section.id }} {
      display: flex;
    }
  </style>
{% else %}
  <style>
    .insta-btn-prev-{{ section.id }},
    .insta-btn-next-{{ section.id }} {
      display: none;
    }
  </style>
{% endif %}

{% if arrow_show_desktop %}
  <style>
    @media(min-width: 1024px) {
      .insta-btn-prev-{{ section.id }},
      .insta-btn-next-{{ section.id }} {
        display: flex;
      }
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {
      .insta-btn-prev-{{ section.id }},
      .insta-btn-next-{{ section.id }} {
        display: none;
      }
    }
  </style>
{% endif %}

{% if scrollbar_show_mobile %}
  <style>
    .insta-scrollbar-{{ section.id }} {
      display: flex !important;
    }
  </style>
{% else %}
  <style>
    .insta-scrollbar-{{ section.id }} {
      display: none !important;
    }
  </style>
{% endif %}

{% if scrollbar_show_desktop %}
  <style>
    @media(min-width: 1024px) {
      .insta-scrollbar-{{ section.id }} {
        display: flex !important;
      }
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {
      .insta-scrollbar-{{ section.id }} {
        display: none !important;
      }
    }
  </style>
{% endif %}

{% comment %} PREVIEW {% endcomment %}
<style>
  .insta-slider-{{ section.id }}.preview .swiper-wrapper {
    display: flex;
    align-items: center;
    justify-content: space-between;
    overflow: hidden;
    gap: {{ slider_gap }}px;
  }

  .insta-slider-{{ section.id }}.preview .insta-slide-{{ section.id }} {
    width: 30%;
  }

  .insta-slider-{{ section.id }}.preview .insta-slide-{{ section.id }}:not(:first-child) {
    margin-left: {{ slider_gap }}px;
  }
</style>

<div class="section-{{ section.id }} insta-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings">

      {% if heading != blank and heading_row == false %}
        <div class="insta-heading-{{ section.id }}">{{ heading }}</div>
      {% endif %}

      <div class="insta-items-{{ section.id }}">

        {% if heading != blank and heading_row %}
          <div class="insta-heading-{{ section.id }}">{{ heading }}</div>
        {% endif %}

        <div class="insta-slider-wrapper-{{ section.id }}">
          <div class="insta-slider-{{ section.id }} swiper preview">
            <div class="swiper-wrapper">
              {% for block in section.blocks %}
                <{% if block.settings.url != blank %}a href="{{ block.settings.url }}"{% else %}div{% endif %} class="insta-slide-{{ section.id }} swiper-slide">
                  
                  <div class="insta-top-{{ section.id }}">
                    {% unless hide_logo %}
                      <div class="insta-logo-{{ section.id }}">
                        {% if logo != blank %}
                          <img src="{{ logo | image_url }}" alt="{{ logo.alt }}" {% if lazy %}lazy="loading"{% endif %}>
                        {% endif %}
                      </div>
                    {% endunless %}
                    <div class="insta-info-{{ section.id }}">
                      {% if author != blank %}
                        <p class="insta-author-{{ section.id }}">{{ author }}</p>
                      {% endif %}
                      {% if block.settings.place != blank %}
                        <p class="insta-place-{{ section.id }}">{{ block.settings.place }}</p>
                      {% endif %}
                    </div>
                    <div class="insta-dots-{{ section.id }}">
                      <svg width="4" height="15" viewBox="0 0 4 15" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1.75 2.5C2.16421 2.5 2.5 2.16421 2.5 1.75C2.5 1.33579 2.16421 1 1.75 1C1.33579 1 1 1.33579 1 1.75C1 2.16421 1.33579 2.5 1.75 2.5Z" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
                        <path d="M1.75 8C2.16421 8 2.5 7.66421 2.5 7.25C2.5 6.83579 2.16421 6.5 1.75 6.5C1.33579 6.5 1 6.83579 1 7.25C1 7.66421 1.33579 8 1.75 8Z" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
                        <path d="M1.75 13.5C2.16421 13.5 2.5 13.1642 2.5 12.75C2.5 12.3358 2.16421 12 1.75 12C1.33579 12 1 12.3358 1 12.75C1 13.1642 1.33579 13.5 1.75 13.5Z" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
                      </svg>
                    </div>
                  </div>
                  
                  <div class="insta-image-{{ section.id }}">
                    {% if block.settings.image != blank %}
