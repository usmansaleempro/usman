  assign author_custom = section.settings.author_custom
  assign author_font = section.settings.author_font
  assign author_height = section.settings.author_height
  assign author_mt = section.settings.author_mt

  assign place_size = section.settings.place_size
  assign place_size_mobile = section.settings.place_size_mobile
  assign place_color = section.settings.place_color
  assign place_custom = section.settings.place_custom
  assign place_font = section.settings.place_font
  assign place_height = section.settings.place_height
  assign place_mt = section.settings.place_mt

  assign image_radius = section.settings.image_radius
  assign image_border_thickness = section.settings.image_border_thickness
  assign image_border_color = section.settings.image_border_color
  assign image_ratio = section.settings.image_ratio
  assign image_ratio_mobile = section.settings.image_ratio_mobile
  assign image_mt = section.settings.image_mt

  assign actions_gap_mobile = section.settings.actions_gap_mobile
  assign actions_gap = section.settings.actions_gap
  assign actions_mt = section.settings.actions_mt
  assign actions_icon_size = section.settings.actions_icon_size
  assign actions_icon_size_mobile = section.settings.actions_icon_size_mobile

  assign time_size = section.settings.time_size
  assign time_size_mobile = section.settings.time_size_mobile
